# The loopback network interface
auto lo
iface lo inet loopback

# First Interface
auto eth0

# Networking for NOVA
auto br100
iface br100 inet static
	bridge_ports    eth0
	bridge_stp      off
	bridge_maxwait  0
	bridge_fd       0
	address <%= ipaddress_eth0 %> 
	netmask <%= netmask_eth0 %> 
	gateway 173.231.145.1



